{
  "rules": {
    "indent": [
      2,
      2,
      { "SwitchCase": 1 }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "windows"
    ],
    "semi": [
      2,
      "always"
    ],
    "eqeqeq": 2,
    "no-console": 0
  },
  "env": {
      "node": true
  },
  "extends": "eslint:recommended"
}
